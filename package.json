{
  "name": "randomgen",
  "version": "0.0.1",
  "description": "Various random generators",
  "author": "Marcin Forseti Pa≈∫dziora",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" -c .eslint/lint-ts.json --fix",
    "lint:dict": "eslint \"dict/**/*.json\" -c ./.eslint/lint-json.json --fix",
    "test": "rimraf dist;jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "rimraf dist; jest --config ./test/e2e/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "9.2.1",
    "@nestjs/platform-express": "9.2.1",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "0.14.0",
    "compression": "^1.7.4",
    "express": "4.18.2",
    "express-rate-limit": "6.7.0",
    "glob": "8.1.0",
    "helmet": "6.0.1",
    "luxon": "3.2.1",
    "nunjucks": "^3.2.3",
    "pino": "8.8.0",
    "random": "4.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "4.1.1",
    "rxjs": "7.8.0",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.8",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.2.1",
    "@tsconfig/node18-strictest": "1.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "4.17.15",
    "@types/jest": "29.2.6",
    "@types/luxon": "3.2.0",
    "@types/node": "18.11.18",
    "@types/nunjucks": "^3.2.1",
    "@types/seedrandom": "3.0.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-json-format": "^2.0.1",
    "growl": "^1.10.5",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "jest-expect-message": "1.1.3",
    "lint-staged": "13.1.0",
    "source-map-support": "^0.5.20",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "eslintIgnore": [
    "dist/",
    "logs/"
  ],
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "npm run lint"
    ],
    "dict/**/*.json": [
      "npm run lint:dict"
    ]
  },
  "ts-node": {
    "transpileOnly": true,
    "files": true,
    "pretty": true
  },
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest",
    "rootDir": "./test",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".(e2e-spec|test).ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "cacheDirectory": ".test/cache",
    "coverageDirectory": ".test/coverage",
    "coverageProvider": "v8",
    "notify": true,
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ]
  }
}
