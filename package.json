{
  "name": "randomgen",
  "version": "0.3.0",
  "description": "Various random generators",
  "author": "Marcin Forseti Pa≈∫dziora",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" -c .eslintrc.json --fix",
    "test": "rimraf dist;jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "rimraf dist; jest --config ./test/e2e/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.8",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.8",
    "@nestjs/platform-express": "^10.2.8",
    "bootstrap": "^5.3.2",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "0.14.0",
    "compression": "^1.7.4",
    "express": "4.18.2",
    "express-rate-limit": "^7.1.4",
    "glob": "^10.3.10",
    "helmet": "^7.1.0",
    "luxon": "3.4.3",
    "marked": "^9.1.5",
    "nunjucks": "^3.2.4",
    "pino": "^8.16.1",
    "random": "4.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "7.8.1",
    "seedrandom": "^3.0.5",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.8",
    "@tsconfig/node20": "20.1.2",
    "@tsconfig/strictest": "2.0.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^6.0.0",
    "@types/jest": "^29.5.8",
    "@types/jest-expect-message": "1.1.0",
    "@types/luxon": "^3.3.4",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.9.0",
    "@types/nunjucks": "^3.2.6",
    "@types/seedrandom": "^3.0.8",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-json-format": "^2.0.1",
    "growl": "^1.10.5",
    "husky": "8.0.3",
    "jest": "^29.7.0",
    "jest-expect-message": "1.1.3",
    "lint-staged": "^15.0.2",
    "node-notifier": "^10.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  },
  "eslintIgnore": [
    "dist/",
    "logs/"
  ],
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "npm run lint"
    ]
  },
  "ts-node": {
    "transpileOnly": true,
    "files": true,
    "pretty": true
  },
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest",
    "rootDir": "./test",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".(e2e-spec|test).ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "cacheDirectory": ".test/cache",
    "coverageDirectory": ".test/coverage",
    "coverageProvider": "v8",
    "notify": true,
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ]
  }
}
