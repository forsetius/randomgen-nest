{
  "name": "randomgen",
  "version": "0.0.1",
  "description": "Various random generators",
  "author": "Marcin Forseti Pa≈∫dziora",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" -c .eslint/lint-ts.json --fix",
    "lint:dict": "eslint \"dict/**/*.json\" -c ./.eslint/lint-json.json --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/e2e/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "^8.3.1",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.3.1",
    "@nestjs/platform-express": "^8.3.1",
    "@nestjs/testing": "^8.3.1",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "express": "^4.17.3",
    "express-rate-limit": "^6.3.0",
    "glob": "^7.2.0",
    "helmet": "^5.0.2",
    "luxon": "^2.3.0",
    "nunjucks": "^3.2.3",
    "pino": "^7.8.0",
    "random": "^3.0.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.4",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.1",
    "@nestjs/schematics": "^8.0.6",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^6.0.0",
    "@types/jest": "^27.4.0",
    "@types/luxon": "^2.0.9",
    "@types/node": "^16.0.0",
    "@types/nunjucks": "^3.2.1",
    "@types/seedrandom": "^3.0.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json-format": "^2.0.1",
    "growl": "^1.10.5",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "node-notifier": "^10.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "eslintIgnore": [
    "dist/",
    "logs/"
  ],
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "npm run lint"
    ],
    "dict/**/*.json": [
      "npm run lint:dict"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest",
    "rootDir": "./test",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".(e2e-spec|test).ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "cacheDirectory": ".test/cache",
    "coverageDirectory": ".test/coverage",
    "coverageProvider": "v8",
    "notify": true
  }
}
