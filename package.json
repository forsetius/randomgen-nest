{
  "name": "randomgen",
  "version": "0.3.0",
  "description": "Various random generators",
  "author": "Marcin Forseti Pa≈∫dziora",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" -c .eslintrc.json --fix",
    "test": "rimraf dist;jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "rimraf dist; jest --config ./test/e2e/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "10.2.4",
    "@nestjs/config": "^3.0.1",
    "@nestjs/core": "10.2.4",
    "@nestjs/platform-express": "10.2.4",
    "bootstrap": "^5.3.2",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "0.14.0",
    "compression": "^1.7.4",
    "express": "4.18.2",
    "express-rate-limit": "6.11.0",
    "glob": "10.3.4",
    "helmet": "7.0.0",
    "luxon": "3.4.3",
    "marked": "^9.0.1",
    "nunjucks": "^3.2.4",
    "pino": "8.15.0",
    "random": "4.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1",
    "seedrandom": "^3.0.5",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.17",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.2.4",
    "@tsconfig/node20": "20.1.2",
    "@tsconfig/strictest": "2.0.2",
    "@types/compression": "^1.7.3",
    "@types/express": "4.17.17",
    "@types/express-rate-limit": "^6.0.0",
    "@types/jest": "29.5.4",
    "@types/jest-expect-message": "1.1.0",
    "@types/luxon": "3.3.2",
    "@types/marked": "^5.0.1",
    "@types/node": "20.5.9",
    "@types/nunjucks": "^3.2.3",
    "@types/seedrandom": "3.0.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "eslint": "8.48.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-json-format": "^2.0.1",
    "growl": "^1.10.5",
    "husky": "8.0.3",
    "jest": "29.6.4",
    "jest-expect-message": "1.1.3",
    "lint-staged": "14.0.1",
    "node-notifier": "^10.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  },
  "eslintIgnore": [
    "dist/",
    "logs/"
  ],
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "npm run lint"
    ]
  },
  "ts-node": {
    "transpileOnly": true,
    "files": true,
    "pretty": true
  },
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest",
    "rootDir": "./test",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".(e2e-spec|test).ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "cacheDirectory": ".test/cache",
    "coverageDirectory": ".test/coverage",
    "coverageProvider": "v8",
    "notify": true,
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ]
  }
}
